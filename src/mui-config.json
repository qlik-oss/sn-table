{
  "palette": {
    "type": "light",
    "primary": {
      "light": "#0AAF54",
      "main": "#009845",
      "dark": "#0D6932",
      "contrastText": "#FFFFFF"
    },
    "secondary": {
      "light": "#469DCD",
      "main": "#3F8AB3",
      "dark": "#2F607B",
      "contrastText": "#FFFFFF"
    },
    "error": {
      "light": "#F05551",
      "main": "#DC423F",
      "dark": "#97322F",
      "contrastText": "#FFFFFF"
    },
    "warning": {
      "light": "#FFC629",
      "main": "#EF960F",
      "dark": "#A4681C",
      "contrastText": "#FFFFFF"
    },
    "success": {
      "light": "#0AAF54",
      "main": "#009845",
      "dark": "#0D6932",
      "contrastText": "#FFFFFF"
    },
    "info": {
      "light": "#469DCD",
      "main": "#3F8AB3",
      "dark": "#2F607B",
      "contrastText": "#FFFFFF"
    },
    "text": {
      "primary": "#404040",
      "secondary": "rgba(0, 0, 0, 0.55)",
      "disabled": "rgba(0, 0, 0, 0.3)",
      "hint": "rgba(0, 0, 0, 0.55)"
    },
    "action": {
      "hover": "rgba(0, 0, 0, 0.03)",
      "hoverOpacity": 0.05
    },
    "background": {
      "paper": "#FFFFFF",
      "default": "#F2F2F2"
    },
    "divider": "rgba(0, 0, 0, 0.15)"
  },
  "props": {
    "MuiButtonBase": {
      "disableRipple": true,
      "disableTouchRipple": true,
      "focusVisibleClassName": "sprout-focus-visible"
    },
    "MuiButton": {
      "focusVisibleClassName": "sprout-focus-visible"
    },
    "MuiSelect": {
      "MenuProps": {
        "anchorOrigin": {
          "vertical": "bottom",
          "horizontal": "left"
        },
        "transformOrigin": {
          "vertical": "top",
          "horizontal": "left"
        },
        "getContentAnchorEl": null
      },
      "variant": "outlined"
    },
    "MuiTableSortLabel": {}
  },
  "spacing": 8,
  "overrides": {
    "MuiButton": {
      "root": {
        "height": "32px",
        "lineHeight": "16px",
        "minWidth": "24px",
        "fontSize": 14,
        "borderRadius": 2,
        "& > $label > i:only-child": {
          "marginLeft": "-9px",
          "marginRight": "-9px"
        }
      },
      "text": {
        "padding": "8px 16px",
        "&:active": {
          "backgroundColor": "rgba(0, 0, 0, 0.1)"
        },
        "&$focusVisible": {
          "boxShadow": "0 0 0 2px #177FE6"
        }
      },
      "outlined": {
        "padding": "7px 16px",
        "borderColor": "rgba(0, 0, 0, 0.15)",
        "backgroundColor": "rgba(255, 255, 255, 0.6)",
        "&:active": {
          "backgroundColor": "rgba(0, 0, 0, 0.1)"
        },
        "&$focusVisible": {
          "borderColor": "#177FE6",
          "boxShadow": "0 0 0 1px #177FE6"
        }
      },
      "contained": {
        "padding": "8px 16px",
        "boxShadow": "none",
        "&:hover": {
          "boxShadow": "none"
        },
        "&:active": {
          "boxShadow": "none"
        }
      },
      "containedPrimary": {
        "&:hover": {
          "backgroundColor": "#0AAF54"
        },
        "&:active": {
          "backgroundColor": "#0D6932"
        },
        "&$focusVisible": {
          "borderColor": "#0AAF54",
          "boxShadow": "0 0 0 3px rgba(10, 175, 84, 0.5)"
        }
      },
      "containedSecondary": {
        "&:hover": {
          "backgroundColor": "#469DCD"
        },
        "&:active": {
          "backgroundColor": "#2F607B"
        },
        "&$focusVisible": {
          "borderColor": "#469DCD",
          "boxShadow": "0 0 0 3px rgba(70, 157, 205, 0.5)"
        }
      },
      "sizeSmall": {
        "fontSize": 12,
        "height": "24px",
        "& > $label > i:only-child": {
          "marginLeft": "-7px",
          "marginRight": "-7px"
        }
      },
      "sizeLarge": {
        "fontSize": 16,
        "height": "40px",
        "lineHeight": "20px",
        "& > $label > i:only-child": {
          "marginLeft": "-10px",
          "marginRight": "-10px"
        }
      },
      "textSizeSmall": {
        "padding": "4px 11px"
      },
      "textSizeLarge": {
        "padding": "10px 19px"
      },
      "outlinedSizeSmall": {
        "padding": "3px 11px"
      },
      "outlinedSizeLarge": {
        "padding": "9px 19px"
      },
      "containedSizeSmall": {
        "padding": "4px 11px"
      },
      "containedSizeLarge": {
        "padding": "10px 19px"
      }
    },
    "MuiSelect": {
      "root": {
        "zIndex": 1,
        "borderRadius": "3px !important",
        "& em": {
          "color": "rgba(0, 0, 0, 0.55)",
          "WebkitFontSmoothing": "antialiased",
          "&::after": {
            "content": ""
          }
        },
        "& ~i": {
          "position": "absolute",
          "right": "12px",
          "padding": "6px"
        },
        "&[aria-expanded=true]": {
          "backgroundColor": "rgba(0, 0, 0, 0.05)"
        },
        "& ~fieldset": {
          "boxShadow": "none !important",
          "border": "none !important",
          "backgroundColor": "unset !important"
        },
        "&:hover": {
          "backgroundColor": "rgba(0, 0, 0, 0.03)"
        },
        "&:active": {
          "backgroundColor": "rgba(0, 0, 0, 0.1)"
        },
        "&:focus": {
          "backgroundColor": "rgba(0, 0, 0, 0.03)",
          "borderRadius": "3px !important",
          "border": "solid 1px #177FE6",
          "boxShadow": "inset 0px 0px 0px 1px #177FE6"
        },
        "&$disabled": {
          "opacity": "unset",
          "color": "rgba(0, 0, 0, 0.3)",
          "&:hover": {
            "backgroundColor": "rgba(0, 0, 0, 0)",
            "borderColor": "rgba(0, 0, 0, 0.15)"
          },
          "& ~i": {
            "color": "rgba(0, 0, 0, 0.3)"
          }
        }
      },
      "selectMenu": {
        "height": "14px"
      },
      "outlined": {
        "border": "solid 1px rgba(0, 0, 0, 0.15)"
      }
    },
    "MuiIconButton": {
      "root": {
        "padding": 7,
        "borderRadius": 2,
        "border": "1px solid transparent",
        "&.sprout-focus-visible": {
          "borderColor": "#177FE6",
          "boxShadow": "0 0 0 1px #177FE6"
        },
        "&&&:hover": {
          "backgroundColor": "transparent"
        }
      }
    },
    "MuiListItem": {
      "root": {
        "height": "48px",
        "&$selected": {}
      },
      "gutters": {
        "paddingLeft": "12px",
        "paddingRight": "12px"
      },
      "button": {
        "&:hover": {
          "backgroundColor": "rgba(0, 0, 0, 0.03)"
        },
        "&:active": {
          "backgroundColor": "rgba(0, 0, 0, 0.05)"
        }
      }
    },
    "MuiTableCell": {
      "root": {
        "padding": "0px 8px 0px 16px",
        "borderBottom": "1px solid #D9D9D9",
        "borderRight": "1px solid #D9D9D9",
        "height": "auto",
        "lineHeight": "130%",
        "&:focus": {
          "boxShadow": "0 0 0 2px #3f8ab3 inset",
          "outline": "none"
        }
      },
      "head": {
        "height": "auto",
        "fontWeight": 600,
        "backgroundColor": "#FAFAFA",
        "lineHeight": "150%"
      },
      "paddingCheckbox": {
        "width": "40px",
        "padding": "0px"
      },
      "sizeSmall": {
        "padding": "0px 8px",
        "height": "29px",
        "&$head": {
          "height": "31px"
        },
        "&$paddingCheckbox": {
          "width": "40px",
          "padding": "0px"
        }
      },
      "stickyHeader": {
        "backgroundColor": "#FAFAFA",
        "borderBottom": "1px solid #D9D9D9"
      }
    },
    "MuiTableContainer": {
      "root": {
        "borderBottom": "1px solid #D9D9D9",
        "borderTop": "1px solid #D9D9D9"
      }
    },
    "MuiTableHead": {
      "root": {
        "backgroundColor": "#FAFAFA"
      }
    },
    "MuiTableRow": {
      "hover": {
        "&$selected": {
          "backgroundColor": "rgba(0, 0, 0, 0.05)",
          "&:hover": {
            "backgroundColor": "rgba(0, 0, 0, 0.03)"
          }
        },
        "&&:hover": {
          "backgroundColor": "rgba(0, 0, 0, 0)"
        }
      }
    },
    "MuiTableSortLabel": {
      "root": {
        "display": "flex",
        "justifyContent": "space-between",
        "&:hover": {
          "& icon": {
            "opacity": 0.5
          },
          "color": "inherit"
        },
        "&$active": {
          "&& icon": {
            "opacity": 1
          },
          "color": "inherit"
        },
        "color": "inherit"
      },
      "icon": {
        "opacity": 0
      },
      "iconDirectionDesc": {
        "transform": "rotate(180deg)"
      },
      "iconDirectionAsc": {
        "transform": "rotate(0deg)"
      }
    },
    "MuiTable": {
      "root": {
        "borderLeft": "1px solid #D9D9D9"
      }
    }
  },
  "typography": {
    "fontSize": 14,
    "fontWeightLight": 300,
    "fontWeightRegular": 400,
    "fontWeightMedium": 600,
    "fontWeightBold": 700,
    "htmlFontSize": 14,
    "fontFamily": "'Source Sans Pro', 'HelveticaNeue', 'Helvetica Neue', Helvetica, Arial, sans-serif",
    "button": {
      "fontWeight": 600,
      "fontSize": 14,
      "textTransform": "none"
    },
    "body1": {
      "fontSize": 16,
      "lineHeight": "24px"
    },
    "body2": {
      "fontSize": 14,
      "lineHeight": "20px"
    },
    "h1": {
      "fontSize": 32,
      "lineHeight": "32px"
    },
    "h2": {
      "fontSize": 28,
      "lineHeight": "32px"
    },
    "h3": {
      "fontSize": 24,
      "lineHeight": "24px"
    },
    "h4": {
      "fontSize": 20,
      "lineHeight": "24px"
    },
    "h5": {
      "fontSize": 16,
      "fontWeight": 600,
      "lineHeight": "16px"
    },
    "h6": {
      "fontSize": 14,
      "fontWeight": 600,
      "lineHeight": "16px"
    },
    "caption": {
      "color": "rgba(0, 0, 0, 0.55)"
    },
    "subtitle1": {
      "fontSize": 14,
      "color": "rgba(0, 0, 0, 0.55)"
    },
    "subtitle2": {
      "fontSize": 12,
      "color": "rgba(0, 0, 0, 0.55)"
    }
  },
  "shape": {
    "borderRadius": 2
  }
}