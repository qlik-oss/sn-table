{
  "name": "@nebula.js/sn-table",
  "version": "1.0.3",
  "description": "table supernova",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "nebula",
    "stardust"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/sn-table.git"
  },
  "files": [
    "dist",
    "api-specifications",
    "core"
  ],
  "engines": {
    "node": ">=14"
  },
  "main": "dist/sn-table.js",
  "scripts": {
    "build": "node ./tools/build.js --core",
    "build:dev": "node ./tools/build.js --core --mode development",
    "build:watch": "node ./tools/build.js --mode development -w",
    "lint": "eslint src",
    "start": "nebula serve --build false --type sn-table",
    "sense": "nebula sense",
    "spec": "scriptappy-from-jsdoc -c ./spec-configs/props.conf.js",
    "test:unit": "aw -c aw.config.js",
    "test:integration": "aw puppet --testExt '*.int.js' --glob 'test/integration/**/*.int.js'",
    "prepublishOnly": "NODE_ENV=production yarn run build",
    "prepack": "./tools/prepare-sn-pack.js"
  },
  "devDependencies": {
    "@after-work.js/aw": "6.0.14",
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@material-ui/core": "^4.12.3",
    "@nebula.js/cli": "1.7.0",
    "@nebula.js/cli-build": "1.7.0",
    "@nebula.js/cli-sense": "1.7.0",
    "@nebula.js/cli-serve": "1.7.0",
    "@testing-library/react": "12.1.0",
    "@testing-library/react-hooks": "7.0.2",
    "babel-eslint": "10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "4.3.4",
    "chai-dom": "1.9.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.12.2",
    "jsdom": "16.7.0",
    "mocha-junit-reporter": "^2.0.0",
    "prettier": "2.4.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "scriptappy-from-jsdoc": "0.7.0",
    "shx": "^0.3.3",
    "sinon": "11.1.2",
    "sinon-chai": "3.7.0"
  },
  "peerDependencies": {
    "@nebula.js/stardust": "^1.7"
  }
}
