{
  "name": "@nebula.js/sn-table",
  "version": "0.2.0",
  "description": "table supernova",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "nebula",
    "stardust"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/sn-table.git"
  },
  "files": [
    "assets",
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "main": "dist/sn-table.js",
  "module": "dist/sn-table.esm.js",
  "scripts": {
    "build": "rollup -c rollup.config.js && shx cp assets/* dist",
    "build:watch": "rollup -c rollup.config.js -w",
    "lint": "eslint src",
    "start": "nebula serve --build false  --type sn-table",
    "sense": "nebula sense",
    "spec": "scriptappy-from-jsdoc -c ./spec-configs/props.conf.js",
    "test:unit": "aw -c aw.config.js",
    "test:integration": "aw puppet --testExt '*.int.js' --glob 'test/integration/**/*.int.js'"
  },
  "devDependencies": {
    "@after-work.js/aw": "6.0.14",
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/plugin-proposal-optional-chaining": "7.13.8",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@material-ui/core": "^4.11.3",
    "@nebula.js/cli": "1.1.0",
    "@nebula.js/cli-build": "1.1.0",
    "@nebula.js/cli-sense": "1.1.0",
    "@nebula.js/cli-serve": "1.1.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@testing-library/react": "11.2.5",
    "@testing-library/react-hooks": "5.1.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "4.3.3",
    "chai-dom": "1.9.0",
    "eslint": "7.21.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-mocha": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.10.1",
    "jsdom": "16.5.0",
    "mocha-junit-reporter": "2.0.0",
    "prettier": "2.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.41.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-visualizer": "^4.2.0",
    "scriptappy-from-jsdoc": "0.7.0",
    "shx": "^0.3.3",
    "sinon": "9.2.4",
    "sinon-chai": "3.5.0",
    "@qlik/sprout-icons": "^0.9.2",
    "@qlik/sprout-theme": "^1.16.0"
  },
  "peerDependencies": {
    "@nebula.js/stardust": "^1.1"
  }
