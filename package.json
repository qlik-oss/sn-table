    "@testing-library/react-hooks": "5.1.2",
    "babel-eslint": "10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "4.3.4",
    "chai-dom": "1.9.0",
    "eslint": "7.25.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
