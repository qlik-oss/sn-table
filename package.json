    "react-dom": "^17.0.2",
    "rollup": "^2.46.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-visualizer": "^5.4.1",
    "sinon": "10.0.0",
    "sinon-chai": "3.6.0",
    "rollup-plugin-terser": "^7.0.2",
    "scriptappy-from-jsdoc": "0.7.0",
